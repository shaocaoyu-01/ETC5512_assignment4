library(tidyverse)
library(here)
library(sf)
library(patchwork)
library(readr)
#pull data
url = "https://data.stats.gov.cn/english/easyquery.htm?cn=E0103"
# this data is about the number of residents in every province
annual_residents <- read.csv(
file = "data/Annual_residents.csv",
header = TRUE,
stringsAsFactors = FALSE,
skip = 3
)
panel_residents <- annual_residents |>
pivot_longer(
cols = -Region,
names_to = "Year",
values_to = "Residents"
)
#pull data
url = "https://data.stats.gov.cn/english/easyquery.htm?cn=E0103"
# this data is about the GDP of each province
annual_gdp <- read.csv(
file = "data/Annual_gdp.csv",
header = TRUE,
stringsAsFactors = FALSE,
skip = 3
)
panel_gdp <- annual_gdp |>
pivot_longer(
cols = -Region,
names_to =   "Year",
values_to = "GDP"
)
panel_all <-
right_join(
panel_gdp,
panel_residents,
by= c("Region","Year")
)
#this  data is the controllable variables
View(panel_all)
View(panel_all)
#pull data
