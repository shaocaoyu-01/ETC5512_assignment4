library(tidyverse)
library(here)
library(sf)
library(patchwork)
#pull data
url = "https://data.stats.gov.cn/english/easyquery.htm?cn=E0103"
# this data is about the number of residents in every province
annual_residents <-
read.csv(
"data/Annual_residents.csv",
skip=3,
stringsAsFactors = FALSE)
annual_residents <- annual_residents[-nrow(annual_residents), ]
panel_residents <-
pivot_longer(
annual_residents,
cols = -"Region",
names_to = "Year",
values_to = "residents"
)
#pull data
url = "https://data.stats.gov.cn/english/easyquery.htm?cn=E0103"
# this data is about the GDP of each province
annual_GDP <-
read.csv(
"data/Annual_gdp.csv",
skip=3,
stringsAsFactors = FALSE)
annual_GDP <- annual_GDP[-nrow(annual_GDP), ]
panel_GDP <-
pivot_longer(
annual_GDP,
cols = -"Region",
names_to = "Year",
values_to = "GDP")
panel_combined <-
right_join(
panel_GDP,
panel_residents,
by = c("Region", "Year")
)
panel_all <-
panel_combined |>
mutate (
GDP_per_capita = GDP/residents)
panel_all <-
panel_all |>
group_by(
Region) |>
arrange(Year) |>
mutate(
growth_rate = (GDP- lag(GDP))/lag(GDP),
growth_rate_per_capita = (GDP_per_capita -lag(GDP_per_capita))/lag(GDP_per_capita))
