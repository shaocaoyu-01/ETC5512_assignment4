file = "data/Annual_gdp.csv",
header = TRUE,
stringsAsFactors = FALSE,
skip = 3
)
panel_gdp <- annual_gdp |>
pivot_longer(
cols = -Region,
names_to =   "Year",
values_to = "GDP"
)
panel_all <-
right_join(
panel_gdp,
panel_residents,
